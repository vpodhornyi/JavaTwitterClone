{
    "env": {
        "browser": true,
        "es2021": true
    },
    "extends": "plugin:react/recommended",
    "overrides": [
    ],
    "parserOptions": {
        "ecmaVersion": "latest",
        "sourceType": "module"
    },
    "plugins": [
        "react"
    ],
    "rules": {
        "no-trailing-spaces": ["error", { "skipBlankLines": true } ],
        "no-return-assign": 0,
        "eol-last": 0
    }
}
