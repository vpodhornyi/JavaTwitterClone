  "scripts": {
    "stylelint-dev": "stylelint 'src/**/*.scss' --cache --cache-location './.stylelintcache'; exit 0",
    "stylelint-build": "stylelint 'src/**/*.scss'",
    "watch-styles-sass": "npm-watch",
    "start-js": "set NODE_OPTIONS=--max_old_space_size=8192 && react-app-rewired start",
    "start": "npm run -p watch-styles-sass start-js",
    "build": "CI=false && npm run stylelint-build && react-app-rewired build",
    "test": "react-app-rewired test",
    "eject": "react-app-rewired eject"
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "devDependencies": {
    "concurrently": "^7.4.0",
    "customize-cra": "^1.0.0",
    "eslint": "^8.25.0",
    "eslint-plugin-react": "^7.31.10",
    "http-proxy-middleware": "^2.0.6",
    "husky": "^8.0.1",
    "lint-staged": "^13.0.3",
    "node-sass": "^7.0.3",
    "npm-watch": "^0.11.0",
    "prettier": "^2.7.1",
    "react-app-rewired": "^2.2.1",
    "standart": "^6.1.0",
    "stylelint": "^14.14.0",
    "stylelint-config-rational-order": "^0.1.2",
    "stylelint-order": "^5.0.0"
  }
